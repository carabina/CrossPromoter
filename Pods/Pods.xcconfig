GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/NSString+Validation" "${PODS_ROOT}/Headers/PureLayout" "${PODS_ROOT}/Headers/UIViewController+StoreKit"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/NSString+Validation" -isystem "${PODS_ROOT}/Headers/PureLayout" -isystem "${PODS_ROOT}/Headers/UIViewController+StoreKit"
OTHER_LDFLAGS = -ObjC -framework Foundation
PODS_ROOT = ${SRCROOT}/Pods